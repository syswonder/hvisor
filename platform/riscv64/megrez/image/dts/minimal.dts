			i-tlb-size = <0x20>;
			mmu-type = "riscv,sv48";
			next-level-cache = <0x0a>;
			reg = <0x03>;
			riscv,isa = "rv64imafdch_zicsr_zifencei_zba_zbb_sscofpmf";
			riscv,pmpgranularity = <0x1000>;
			riscv,pmpregions = <0x08>;
			sifive,buserror = <0x0b>;
			status = "okay";
			timebase-frequency = <0xf4240>;
			tlb-split;
