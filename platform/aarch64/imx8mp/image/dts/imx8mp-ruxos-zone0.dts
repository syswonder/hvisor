			rpm-always-on;
			clocks = <0x03 0x10e 0x03 0x10d>;
			phandle = <0x0f>;
		};

		power-domain@14 {
			compatible = "fsl,imx8m-pm-domain";
			#power-domain-cells = <0x00>;
			domain-index = <0x0e>;
			domain-name = "ispdwp";
			parent-domains = <0x0f>;
			clocks = <0x03 0x5c>;
