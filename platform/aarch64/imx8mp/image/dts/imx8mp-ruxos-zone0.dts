		bus@30c00000 {
			compatible = "simple-bus";
			reg = <0x30c00000 0x400000>;
			#address-cells = <0x01>;
			#size-cells = <0x01>;
			ranges;
		
			spba-bus@30c00000 {
				compatible = "fsl,spba-bus\0simple-bus";
